var jsonSchema = {};
{% for ns in schemas %}
jsonSchema["{{ns}}"] = {};
    {% for method, schema in schemas[ns].iteritems() %}
jsonSchema["{{ns}}"]["{{method}}"] = {{schema | json_dumps}};
    {% endfor %}
{% endfor %}

if (typeof exports !== 'undefined') {
    if (typeof module !== 'undefined' && module.exports) {
        exports = module.exports = jsonSchema;
    }
    exports.jsonSchema = jsonSchema;
}

